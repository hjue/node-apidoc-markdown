{
  "name": "apidocjs-markdown",
  "version": "0.2.1",
  "description": "Generate API documentation in markdown format from apidoc data.",
  "main": "index.js",
  "scripts": {
    "test": "npm run jshint",
    "jshint": "jshint index.js",
    "release": "release-it -n -i patch",
    "release:minor": "release-it -n -i minor",
    "release:major": "release-it -n -i major"
  },
  "directories": {
    "example": "examples"
  },
  "bin": {
    "apidocjs-markdown": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hjue/node-apidoc-markdown"
  },
  "keywords": [
    "apidoc",
    "markdown"
  ],
  "author": "Jason",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjue/node-apidoc-markdown/issues"
  },
  "dependencies": {
    "optimist": "~0.6.0",
    "underscore": "~1.5.2",
    "ejs": "~0.8.5"
  },
  "devDependencies": {
    "release-it": "0.0.12"
  }
}
